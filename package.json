{
  "name": "@f8n/electric",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "next build",
    "clean": "find . -type f -name '*.generated.*' -exec rm {} +",
    "clean:dry": "find . -type f -name '*.generated.*'",
    "env:mainnet": "pnpm run project:mainnet && pnpm vercel env pull .env.local.mainnet && cp .env.local.mainnet .env.local",
    "env:testnet": "pnpm run project:testnet && pnpm vercel env pull .env.local.testnet && cp .env.local.testnet .env.local",
    "dev": "next dev",
    "dev:mainnet": "cp .env.local.mainnet .env.local && next dev",
    "dev:testnet": "cp .env.local.testnet .env.local && next dev",
    "frog": "frog dev",
    "generate": "pnpm generate:api",
    "generate:api": "pnpm generate:electric-v2 && pnpm generate:marketplace",
    "generate:electric-v2": "pnpm generate:electric-v2-schema && pnpm generate:electric-v2-react-query && pnpm generate:electric-v2-types && pnpm generate:electric-v2-msw && pnpm generate:electric-v2-mocks && pnpm generate:electric-v2-sdk",
    "generate:electric-v2-mocks": "graphql-codegen --config codegen/gql-electric-v2-mocks.ts",
    "generate:electric-v2-msw": "graphql-codegen --config codegen/gql-electric-v2-msw.ts",
    "generate:electric-v2-react-query": "graphql-codegen --config codegen/gql-electric-v2-react-query.ts",
    "generate:electric-v2-schema": "graphql-codegen --config codegen/gql-electric-v2-schema.ts",
    "generate:electric-v2-sdk": "graphql-codegen --config codegen/gql-electric-v2-sdk.ts",
    "generate:electric-v2-types": "graphql-codegen --config codegen/gql-electric-v2-types.ts",
    "generate:marketplace": "pnpm generate:marketplace-schema && pnpm generate:marketplace-react-query && pnpm generate:marketplace-types",
    "generate:marketplace-react-query": "graphql-codegen --config codegen/gql-marketplace-react-query.ts",
    "generate:marketplace-schema": "graphql-codegen --config codegen/gql-marketplace-schema.ts",
    "generate:marketplace-types": "graphql-codegen --config codegen/gql-marketplace-types.ts",
    "generate:panda": "panda codegen",
    "generate:panda-watch": "panda codegen --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:gql": "eslint src/gql",
    "project:mainnet": "vercel link --yes --scope=f8n --project=fnd-electric",
    "project:testnet": "vercel link --yes --scope=f8n --project=fnd-electric-testnet",
    "start": "next start",
    "storybook": "pnpm generate:panda-watch & storybook dev -p 6002",
    "test": "vitest",
    "types": "tsc",
    "build-storybook": "storybook build",
    "prepare": "pnpm generate:panda"
  },
